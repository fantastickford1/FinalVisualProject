            <ContextMenu>
              <items>
                <MenuItem fx:id="eliminarContext" mnemonicParsing="false" onAction="#deleteFile" text="Eliminar" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TreeView>
      <TabPane layoutX="206.0" layoutY="27.0" prefHeight="330.0" prefWidth="499.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Utilidades">
               <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="290.0" prefWidth="499.0">
                     <children>
                        <Button fx:id="btnPaint" layoutX="105.0" layoutY="142.0" mnemonicParsing="false" onAction="#openPaint" prefHeight="43.0" prefWidth="106.0" />
