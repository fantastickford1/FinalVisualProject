                        <TextField fx:id="direcField" layoutX="147.0" layoutY="110.0" opacity="0.68" />
                        <Label layoutX="78.0" layoutY="149.0" text="Telefono" />
                        <TextField fx:id="phoneField" layoutX="147.0" layoutY="145.0" opacity="0.68" />
                        <Label layoutX="101.0" layoutY="190.0" text="Curp" />
                        <TextField fx:id="curpField" layoutX="147.0" layoutY="186.0" opacity="0.68" />
                        <Button fx:id="modificarButton" layoutX="235.0" layoutY="262.0" mnemonicParsing="false" onAction="#Update" text="Modificar" />
                        <TextField fx:id="mailField" layoutX="147.0" layoutY="221.0" opacity="0.68" />
                        <Label layoutX="97.0" layoutY="225.0" text="E-mail" />
                        <Button layoutX="140.0" layoutY="262.0" mnemonicParsing="false" onAction="#Perfil" text="Ver mi perfil" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="706.0">
        <menus>
          <Menu mnemonicParsing="false" text="Archivo">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Editar">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ayuda">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#creditsWindow" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label layoutX="556.0" layoutY="356.0" text="Usuario">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ImageView fitHeight="79.0" fitWidth="76.0" layoutX="610.0" layoutY="309.0">
         <image>
            <Image url="@../img/usuario.png" />
         </image>
      </ImageView>
   </children>
