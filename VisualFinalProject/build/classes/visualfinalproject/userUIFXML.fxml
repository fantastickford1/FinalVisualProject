                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <ImageView fitHeight="58.0" fitWidth="62.0" layoutX="406.0" layoutY="14.0">
                           <image>
                              <Image url="@../img/herramientas.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </content>
          </Tab>
          <Tab text="Panel de Control">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="76.0" layoutY="42.0" text="Nombre" />
                        <TextField fx:id="nameField" layoutX="147.0" layoutY="38.0" opacity="0.68" />
                        <Label layoutX="76.0" layoutY="79.0" text="Apellido" />
                        <TextField fx:id="apellidoField" layoutX="147.0" layoutY="75.0" opacity="0.68" />
